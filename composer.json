{
    "name": "antoninmasek/simple-hydrator",
    "description": "Simple object hydrator",
    "license": "MIT",
    "keywords": [
        "antoninmasek",
        "simple-hydrator"
    ],
    "authors": [
        {
            "name": "Antonín Mašek",
            "email": "antonin.masek@icloud.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/antoninmasek/simple-hydrator",
    "require": {
        "php": "^8.2",
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "spatie/ray": "^1.32.2"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AntoninMasek\\SimpleHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntoninMasek\\SimpleHydrator\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "scripts": {
        "lint": "vendor/bin/pint",
        "test": "vendor/bin/phpunit --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage"
    }
}
