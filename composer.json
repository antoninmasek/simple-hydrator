{
    "name": "antoninmasek/simple-hydrator",
    "description": "Simple object hydrator",
    "license": "MIT",
    "keywords": [
        "antoninmasek",
        "simple-hydrator"
    ],
    "authors": [
        {
            "name": "Antonín Mašek",
            "email": "antonin.masek@icloud.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/antoninmasek/simple-hydrator",
    "require": {
        "php": "^8.0|^8.1",
        "phpunit/phpunit": "^9.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "spatie/ray": "^1.28"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AntoninMasek\\SimpleHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntoninMasek\\SimpleHydrator\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    }
}
